  <%= form_for [:admin,@address] do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <p>Owner ID: <%= @user.id %>
  <p>Owner Name: <%= link_to "#{@user.first_name} #{@user.last_name} (click to view)", admin_user_path(@user) %></p>

  <%= f.label :user_id %>
  <%= f.text_field :user_id, :value => @user.id, :readonly => true %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit %>

  <% end %>
